{
  "name": "@ludlovian/filecache",
  "version": "0.2.3",
  "description": "Cache of files",
  "main": "src/index.mjs",
  "exports": {
    ".": "./src/index.mjs"
  },
  "scripts": {
    "clean": "rm -rf coverage/ node_modules/.cache/",
